{
  "name": "react-image-carousel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "concurrently -k \"npm run front\" \"npm run db\"",
    "db": "json-server --watch api/movies.json --port 3005",
    "front": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "json-server": "^0.14.0"
  }
}
